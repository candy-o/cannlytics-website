{
  "css.lint.unknownProperties": "ignore",
  "editor.detectIndentation": false,
  "editor.glyphMargin": false,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "strings": true,
  },
  "files.eol": "\n",
  "files.watcherExclude": {
    "**/app/build/win-unpacked/**": true
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/__init__.py": true,
  },
  "git.enabled": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "git.decorations.enabled": false,
  "git.ignoreLimitWarning": true,
  "python.linting.pylintEnabled": true,
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4  
  },
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "foreground": "#0BA67F",
      "iconColour": "green",
      "rulerColour": "green",
      "rulerLane": "left"
    },
    "FIXME": {
      "foreground": "orange",
      "iconColour": "red",
      "rulerColour": "red",
      "rulerLane": "left"
    }
  },
  "cSpell.words": [
    "abcdefghijklmnopqrstuvwxyz",
    "analytes",
    "appspot",
    "arithmatex",
    "autohide",
    "boxplot",
    "cannlypedia",
    "cannlytics",
    "chartjs",
    "checkmark",
    "cinzel",
    "cloudbuild",
    "cloudrun",
    "cloudsql",
    "coas",
    "codehilite",
    "collectstatic",
    "daygrid",
    "djpass",
    "djuser",
    "esmodules",
    "firebaseapp",
    "firestore",
    "fullcalendar",
    "gcloud",
    "gemoji",
    "gmail",
    "grotesk",
    "heatmap",
    "inkscape",
    "keegan",
    "keychain",
    "lims",
    "linkedin",
    "makemigrations",
    "metrc",
    "mortle",
    "nbsp",
    "noinput",
    "octicons",
    "paletton",
    "plex",
    "popperjs",
    "postgres",
    "projectnum",
    "pymdownx",
    "roadmap",
    "runserver",
    "scrollable",
    "secretmanager",
    "skeate",
    "sqladmin",
    "staticfiles",
    "subcollections",
    "timegrid",
    "urandom",
    "wsgi",
    "zondicons"
  ],
  "yaml.customTags": [
    "!ref"
  ],
  "todo-tree.tree.showBadges": false,
  "cSpell.ignoreWords": [
    "hawcons"
  ]
}
